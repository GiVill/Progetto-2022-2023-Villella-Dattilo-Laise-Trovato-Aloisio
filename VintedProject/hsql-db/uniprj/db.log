create table basicinsertion (id bigint not null, condition varchar(255), creation_date date, description varchar(255), end_date date, image blob, price integer, title varchar(255), user_id bigint, primary key (id))
create table buyingoffer (id bigint generated by default as identity (start with 1), price float(24), status varchar(255), basic_insertion_id bigint, user_id bigint, primary key (id))
create table dressinsertion (id bigint not null, condition varchar(255), creation_date date, description varchar(255), end_date date, image blob, price integer, title varchar(255), user_id bigint, brand smallint, category varchar(255), primary key (id))
create table favorite (id bigint generated by default as identity (start with 1), insertion_id bigint, user_id bigint, primary key (id))
create table hibernate_sequences (sequence_name varchar(255) not null, next_val bigint, primary key (sequence_name))
