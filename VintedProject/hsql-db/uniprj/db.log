create table user (id bigint generated by default as identity (start with 1), cap integer, city varchar(255), number integer, region varchar(255), state varchar(255), street varchar(255), birth_date date, email varchar(255), firstname varchar(255), gender varchar(255), lastname varchar(255), password varchar(255), phone_number integer, photo blob, registration_date date, role varchar(255), primary key (id))
